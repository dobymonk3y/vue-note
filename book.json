{
  "title": "vue-note",
  "description": "vue-note",
  "language": "zh-hans",
   "plugins": [
       "splitter",
       "-github",
       "prism",
       "-highlight",
       "expandable-chapters",
       "page-footer-ex",
       "anchor-navigation-ex",
       "advanced-emoji",
	   "bootstrap-callout"
    ],
  "pluginsConfig": {
    "prism": {
       "css": [
         "prismjs/themes/prism-okaidia.css"
       ]
    },
    "github-buttons": {
      "repo": "zq99299/gitbook-guide",
      "types": [
        "star",
        "watch"
      ],
      "size": "large"
    },
	"anchor-navigation-ex":{
	  "multipleH1":false
	}
  }
} 