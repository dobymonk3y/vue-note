{
  "title": "vue-note",
  "description": "vue-note",
  "language": "zh-hans",
   "plugins": [
       "splitter",
       "emphasize",
       "github",
       "-toggle-chapters",
       "prism",
       "-highlight",
       "-toc",
       "atoc",
       "-anchor-navigation",
       "-codeblock-filename" ,
       "-sectionx",
       "anchors",
       "-github-buttons",
       "-book-summary-scroll-position-saver",
       "tbfed-pagefooter",
       "expandable-chapters"  
    ],
  "pluginsConfig": {
    "prism": {
       "css": [
         "prismjs/themes/prism-okaidia.css"
       ]
    },
    "disqus": {
      "shortName": "gitbook-guide"
    },
    "github": {
      "url": "https://github.com/zq99299/gitbook-guide"
    },
    "github-buttons": {
      "repo": "zq99299/gitbook-guide",
      "types": [
        "star",
        "watch"
      ],
      "size": "large"
    },
    "tbfed-pagefooter": {
        "copyright":"Copyright &copy mrcode.cn",
        "modify_label": "该文件修订时间：",
        "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "toc": {
        "addClass": true,
        "className": "toc"
    },
    "atoc": {
        "addClass": true,
        "className": "atoc"
    },
    "expandable-chapters":{}
  }
} 